#lang scheme

(provide (all-defined-out))

;-------------------------------------------------------------------------------
;-------------------------------------------------------------------------------

(define startsWith (lambda (str start_str_search)
  (define map_list (map string (string->list str)))
  (define result #f)
  (define str_test "")
  (for ((x map_list))
    (set! str_test (string-append str_test x))
    (if (equal? str_test start_str_search)
      (set! result #t) void
    )
  )
  result
))

